{
  "mcpServers": {
    "cloudflare": {
      "command": "cmd",
      "args": ["/c", "npx", "mcp-remote", "https://docs.mcp.cloudflare.com/sse"]
    },
    "mastra": {
      "command": "cmd",
      "args": ["/c", "npx", "-y", "@mastra/mcp-docs-server"],
      "type": "stdio"
    },
    "serena": {
      "command": "cmd",
      "args": [
        "/c",
        "uvx",
        "--from",
        "git+https://github.com/oraios/serena",
        "serena-mcp-server.exe"
      ]
    }
  }
}
