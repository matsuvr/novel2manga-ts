# LLM API Keys
OPENAI_API_KEY=your-openai-api-key-here
GEMINI_API_KEY=your-gemini-api-key-here
GOOGLE_API_KEY=your-google-api-key-here  # GeminiのAPIキーの別名
GROQ_API_KEY=your-groq-api-key-here

# Cloudflare設定（本番環境用）
CF_ACCOUNT_ID=your-account-id
CF_API_TOKEN=your-api-token

# LLMプロバイダー設定（オプション）
LLM_PROVIDER=gemini  # openai, gemini, groq から選択
OPENAI_MODEL=gpt-4o-mini
GEMINI_MODEL=gemini-2.0-flash-exp
GROQ_MODEL=qwen2.5-coder-32b-instruct
LLM_TEMPERATURE=0.7

# チャンク設定のオーバーライド（オプション）
CHUNK_SIZE=5000
OVERLAP_SIZE=500

# 処理設定のオーバーライド（オプション）
MAX_CONCURRENT_CHUNKS=5

# フィーチャーフラグ（オプション）
ENABLE_CACHING=true
ENABLE_TEXT_ANALYSIS=true

# 環境設定
NODE_ENV=development